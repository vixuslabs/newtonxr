{
  "name": "@vixuslabs/newtonxr",
  "version": "0.2.1",
  "license": "MIT",
  "type": "module",
  "description": "a webxr library for effortlessly infusing realistic physics into your projects",
  "keywords": [
    "webxr",
    "physics",
    "ar",
    "vr",
    "xr"
  ],
  "bugs": {
    "url": "https://github.com/vixuslabs/newtonxr/issues"
  },
  "files": [
    "/dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm run build --watch",
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings 0",
    "clean": "git clean -xdf dist node_modules",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@coconut-xr/natuerlich": "^0.0.48",
    "@react-three/rapier": "1.3.0-canary.0",
    "zustand": "^4.0.0"
  },
  "peerDependencies": {
    "@react-three/fiber": "*",
    "react": "*",
    "three": "*"
  },
  "devDependencies": {
    "@coconut-xr/koestlich": "^0.3.12",
    "@coconut-xr/xinteraction": "^0.1.12",
    "@dimforge/rapier3d-compat": "^0.12.0",
    "@newtonxr/eslint-config": "0.1.0",
    "@newtonxr/tsconfig": "0.1.0",
    "@react-three/fiber": "^8.15.11",
    "@types/react": "^18.2.37",
    "@types/three": "^0.158.2",
    "@types/webxr": "^0.5.8",
    "eslint": "^8.54.0",
    "react": "^18.2.0",
    "three": "^0.158.0",
    "tsup": "8.0.1",
    "typescript": "^5.3.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@newtonxr/eslint-config/base",
      "@newtonxr/eslint-config/react"
    ]
  },
  "prettier": "../../prettier.config.cjs"
}
